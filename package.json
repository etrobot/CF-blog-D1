{
  "name": "cursorhustle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "pnpm run pages:build && wrangler pages dev",
    "deploy": "pnpm run pages:build && wrangler pages deploy",
    "sqlite:gen": "drizzle-kit generate --config drizzle.config.sqlite.ts",
    "sqlite:push": "drizzle-kit push --config drizzle.config.sqlite.ts",
    "sqlite:studio": "drizzle-kit studio --config drizzle.config.sqlite.ts",
    "db:pull": "wrangler d1 export ai-directory --remote --output=backup.sql",
    "db:push": "wrangler d1 execute ai-directory --file=backup.sql",
    "db:migrate": "wrangler d1 migrations apply ai-directory --preview"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@libsql/client": "^0.14.0",
    "@next/third-parties": "^15.0.4",
    "drizzle-orm": "^0.37.0",
    "lucide-react": "^0.468.0",
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.6",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.16.0",
    "drizzle-kit": "^0.29.1",
    "eslint": "^9.15.0",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2"
  }
}
